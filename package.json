{
  "name": "natours",
  "version": "0.0.1",
  "description": "A sample landing page built on pure HTML and CSS",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css --watch ./sass/",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vipr0/Natours.git"
  },
  "author": "Vitalii Protsyk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vipr0/Natours/issues"
  },
  "homepage": "https://github.com/vipr0/Natours#readme",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "concat": "^1.0.3",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  }
}
